{
	"name": "PersonalityEmulator (docker-compose)",
	"dockerComposeFile": ["../docker/docker-compose.yml"],
	"service": "personality_emulator_dev_service",
	"workspaceFolder": "/workspace",
	"shutdownAction": "stopCompose",
	"runServices": ["personality_emulator_dev_service"],
	"forwardPorts": [
		50021,
		50030,
		50050,
		50100,
		50101
	],
	"extensions": [
        "ms-python.python",
        "ms-vscode-remote.vscode-remote-extensionpack",
        "ms-python.autopep8",
        "ms-vscode.cpptools-extension-pack",
        "mhutchie.git-graph",
        "xaver.clang-format"
	],
	"settings": {
		"terminal.integrated.shell.linux": "/bin/bash"
	},
	"remoteUser": "root",
	"postStartCommand": "echo 'Container ready.'"
}